{
    "headers": [
        {
            "source": "**/*.js",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "text/javascript"
                }
            ]
        },
        {
            "source": "**/*.ts",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/typescript"
                }
            ]
        }
    ]
}